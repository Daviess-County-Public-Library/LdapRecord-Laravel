{
    "name": "directorytree/ldaprecord-laravel",
    "description": "LDAP Authentication & Management for Laravel.",
    "keywords": ["ldaprecord", "adldap2", "ldap", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "directorytree/ldaprecord": "*",
        "illuminate/support": "~5.5|~6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.7"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "LdapRecord\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LdapRecord\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LdapRecord\\Laravel\\LdapRecordServiceProvider",
                "LdapRecord\\Laravel\\LdapRecordAuthServiceProvider"
            ]
        }
    }
}
